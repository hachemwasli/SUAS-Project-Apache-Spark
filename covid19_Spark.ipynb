    "## fonction\n",
    "def splitter(lineOfText): \n",
    "     words = lineOfText.split(\" \") \n",
    "     return len(words) \n",
    "def aggregate(numWordsLine1, numWordsLineNext): \n",
    "     totalWords = numWordsLine1 + numWordsLineNext \n",
    "     return totalWords"
   ]
